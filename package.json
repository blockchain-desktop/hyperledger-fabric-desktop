{
  "name": "hyperledger-fabric-desktop",
  "productName": "Fabric-Desktop",
  "version": "0.3.0-alpha",
  "description": "My Electron application description",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --cache --color --ext .jsx,.js src",
    "lintFix": "eslint --fix",
    "test": "jest"
  },
  "jest": {
    "transform": {
      "^.+\\.(js|jsx)?$": "babel-jest"
    }
  },
  "keywords": [],
  "author": "dengyi",
  "license": "GPL-3.0-or-later",
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "npm",
        "ignore": [
          ".idea|doc",
          "fabric/.*",
          "test/.*",
          "resources/key/tx/((?!configtxgen_).)+$",
          "resources/key/users/.*",
          "resources/persistence/.*",
          "resources/styles/image/logo/logo.ai"
        ],
        "icon": "resources/styles/image/logo/logo.icns"
      },
      "electronWinstallerConfig": {
        "name": "hyperledger_fabric_desktop"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "",
        "name": ""
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "hyperledgerfabricdesktop"
      }
    }
  },
  "dependencies": {
    "antd": "^3.9.2",
    "electron-compile": "^6.4.3",
    "electron-devtools-installer": "^2.1.0",
    "electron-log": "^3.0.0-beta",
    "electron-squirrel-startup": "^1.0.0",
    "fabric-ca-client": "~1.3.0",
    "fabric-client": "~1.3.0",
    "grpc": "^1.24.9",
    "js-yaml": "~3.12.0",
    "log4js": "^3.0.6",
    "nedb": "^1.8.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-hot-loader": "^3.0.0-beta.6"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-es2015-classes": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "electron-forge": "^5.2.2",
    "electron-prebuilt-compile": "2.0.7",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^15",
    "eslint-plugin-import": "^2",
    "eslint-plugin-jsx-a11y": "^5",
    "eslint-plugin-react": "^7",
    "@types/jest": "^23.3.13",
    "eslint-plugin-jest": "^22.3.0",
    "jest": "^23.6.0"
  }
}
